backrtrack:
While (res& != 0 && i < 16)
{
	res& = tetriA & tetriB;
	if res& == 0
		break ;
	tetriB != 1 << 1;
	i++;
}
if (i != 16)
{
	newmap = tetriA | tetriB;
	place TetriB -> map >> i;
}
else
{
	undo TetriA;
	i = 0;
	replace TetriA >> 1;
	if (backtrack() == 0)
		print_map()
	else
		free_map
		size++
		backtrack()
}

